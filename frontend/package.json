{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Sabka bazaar UI",
  "main": "index.js",
  "scripts": {
    "analyze:watch": "chokidar webpack.config.prod.js src --initial --ignore .git --ignore node_modules -c \"webpack -c webpack.config.prod.js --env analyze=true --mode=production\"",
    "analyze": "webpack -c webpack.config.prod.js --env analyze=true --mode=production",
    "build": "webpack -c webpack.config.prod.js --mode=production",
    "dev": "webpack serve -c webpack.config.dev.js --mode=development",
    "format": "prettier --write ./src/",
    "lint": "eslint ./src/ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test-coverage": "jest --coverage --watchAll=false"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.2.1",
    "chokidar-cli": "^3.0.0",
    "core-js": "^3.39.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv": "^16.4.7",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "axios": "^1.7.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.0",
    "react-router-dom": "^7.1.0"
  }
